{
  "settings": {
    "eslint.packageManager": "yarn"
  },
  "plugins": ["prettier"],
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "no-console": 0,
    "arrow-parens": "off",
    "implicit-arrow-linebreak": "off",
    "no-confusing-arrow": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
